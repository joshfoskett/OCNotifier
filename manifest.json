{
  "name": "OCNotifier (Dev)",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Get alerts when your favorite maps are playing on OCN.",
  "homepage_url": "https://ocn.rastalulz.com/",
  "icons": {
    "16": "img/icons/icon16.png",
    "48": "img/icons/icon48.png",
    "128": "img/icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "js/localstoragedb.min.js",
      "js/global.js",
      "js/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "img/icons/icon128.png",
    "default_title": "OCNotifier",
    "default_popup": "html/popup.html"
  },
  "permissions": [
    "notifications",
    "https://ocn.rastalulz.com/*",
    "*://*.oc.tc/*"
  ]
}